# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Qdrant Configuration
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=tarento_documents

# JWT Configuration
SECRET_KEY=your-super-secret-key-change-in-production
